\\Napisz program, który tworzy listę tablicową 10 liczb całkowitych, a następnie wyświetla je w konsoli w porządku odwrotnym do wprowadzenia.


import java.util.ArrayList;
import java.util.Scanner;


public class zad1 {
    public static void main(String[] args){
        ArrayList<Integer> LiczbyCalkowite = new ArrayList<>();
        System.out.println("Wprowadz 10 liczb: ");
        Scanner scanner = new Scanner(System.in);
        for (int i =1;i <= 10; i++){
            System.out.print("Wprowadz liczbe nr : "+ i+":");
            LiczbyCalkowite.add(scanner.nextInt());
        }
        for (int i = LiczbyCalkowite.size() -1;i>=0;i--){
            System.out.println(LiczbyCalkowite.get(i));
        }
    }


}
